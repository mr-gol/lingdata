    <TIER ANNOTATOR="Голубцова Мария" DEFAULT_LOCALE="en"
        LINGUISTIC_TYPE_REF="translation" PARENT_REF="text@up1921f"
        PARTICIPANT="@up1921f" TIER_ID="trans@up1921f">
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a61" ANNOTATION_REF="a1">
                <ANNOTATION_VALUE>It's against the mosquitoes.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a62" ANNOTATION_REF="a2">
                <ANNOTATION_VALUE>The mosquitoes bite hard in the summer, they won't let sleep.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a63" ANNOTATION_REF="a3">
                <ANNOTATION_VALUE>Yeah, well, adjust, they adjust poorly.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a64" ANNOTATION_REF="a4">
                <ANNOTATION_VALUE>You can fall asleep, then it bites, it bites.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a65" ANNOTATION_REF="a5">
                <ANNOTATION_VALUE>You can cook shchi, porridge, potatoes on the stove.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a66" ANNOTATION_REF="a7">
                <ANNOTATION_VALUE>Everything is tastier in that stove.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a67" ANNOTATION_REF="a8">
                <ANNOTATION_VALUE>Kurshaks are good working people.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a68" ANNOTATION_REF="a9">
                <ANNOTATION_VALUE>It was some kind of a holiday, uh, let's build a good club for the young people, so that our young people'd go to the club.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a69" ANNOTATION_REF="a10">
                <ANNOTATION_VALUE>And somewhere far away, oh, the forest is burning. Well, it burns, we'll extinguish it. It burns, it burns.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a70" ANNOTATION_REF="a11">
                <ANNOTATION_VALUE>They say, a ball of fire was flying somewhere from the forest. It flew right to this club.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a71" ANNOTATION_REF="a12">
                <ANNOTATION_VALUE>And Kursha-2... So many people were burnt, oh, so many people.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a72" ANNOTATION_REF="a13">
                <ANNOTATION_VALUE>Everyone, someone climbed into the well trying to escape, but there was no escape, everything around was burning, everything around was burning.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a73" ANNOTATION_REF="a14">
                <ANNOTATION_VALUE>Thousads, thousands.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a74" ANNOTATION_REF="a15">
                <ANNOTATION_VALUE>Yes.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a75" ANNOTATION_REF="a16">
                <ANNOTATION_VALUE>We were afraid, we were afraid. We, you know, didn't sleep at home,</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a76" ANNOTATION_REF="a17">
                <ANNOTATION_VALUE>uh, we took out our clothes, laid it on the grass, and we were laying there.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a77" ANNOTATION_REF="a18">
                <ANNOTATION_VALUE>We lay, lay, get cold, then we press close against each other and fall asleep.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a78" ANNOTATION_REF="a19">
                <ANNOTATION_VALUE>This is how we were sleeping during this fire.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a79" ANNOTATION_REF="a20">
                <ANNOTATION_VALUE>There was a barrack, and an old man, he got an ikon.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a80" ANNOTATION_REF="a21">
                <ANNOTATION_VALUE>With this ikon he walked around the barrack three times, it was against this fire.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a81" ANNOTATION_REF="a22">
                <ANNOTATION_VALUE>Everything around was burnt, everything, this barrack stayed.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a82" ANNOTATION_REF="a23">
                <ANNOTATION_VALUE>And these old men, they all stayed, this is how the fire was.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
    </TIER>
    <TIER ANNOTATOR="Голубцова Мария" DEFAULT_LOCALE="en"
        LINGUISTIC_TYPE_REF="translation" PARENT_REF="text@av1980m"
        PARTICIPANT="@av1980m" TIER_ID="trans@av1980m">
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a83" ANNOTATION_REF="a6">
                <ANNOTATION_VALUE>They say that people here adjust to mosquitoes.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a84" ANNOTATION_REF="a27">
                <ANNOTATION_VALUE>And from the first words we discover the tragic fate of Kurshaks.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a85" ANNOTATION_REF="a28">
                <ANNOTATION_VALUE>It turns out that almost all of them burnt to death in the fire of 1936.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a86" ANNOTATION_REF="a29">
                <ANNOTATION_VALUE>And how many people died then?</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a87" ANNOTATION_REF="a30">
                <ANNOTATION_VALUE>Ulyana Pavlovna, it turns out that you were 15 then.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a88" ANNOTATION_REF="a31">
                <ANNOTATION_VALUE>How do you remember that? Were you afraid that the fire will come here?</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a89" ANNOTATION_REF="a32">
                <ANNOTATION_VALUE>Here is the hutch.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a90" ANNOTATION_REF="a33">
                <ANNOTATION_VALUE>Tell me, roughly how many people live now in your village?</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a91" ANNOTATION_REF="a34">
                <ANNOTATION_VALUE>So this village is quite big. How many people were here before?</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a92" ANNOTATION_REF="a35">
                <ANNOTATION_VALUE>And the memory is fading too. Those who knew the Kurshaks, who could tell us about the fire of 1936, it's probably impossible to find them.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a93" ANNOTATION_REF="a54">
                <ANNOTATION_VALUE>And no one survived?</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
    </TIER>
    <TIER ANNOTATOR="Голубцова Мария" DEFAULT_LOCALE="en"
        LINGUISTIC_TYPE_REF="translation" PARENT_REF="text@ny1940m"
        PARTICIPANT="@ny1940m" TIER_ID="trans@ny1940m">
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a94" ANNOTATION_REF="a36">
                <ANNOTATION_VALUE>Kurshaks... I heard that before they lived</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a95" ANNOTATION_REF="a37">
                <ANNOTATION_VALUE>on the Curonian Spit, it's somewhere in the Baltic region.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a96" ANNOTATION_REF="a38">
                <ANNOTATION_VALUE>Our tsar moved them frome there. When he went there he saw that they live really bad,</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a97" ANNOTATION_REF="a39">
                <ANNOTATION_VALUE>they were eating raw fish, well, they ate fish, their houses were old and poor.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a98" ANNOTATION_REF="a40">
                <ANNOTATION_VALUE>And he... Our place was uninhabited, and he moved them here, he felt sorry for them.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a99" ANNOTATION_REF="a41">
                <ANNOTATION_VALUE>Since then these people live here.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a100" ANNOTATION_REF="a42">
                <ANNOTATION_VALUE>So, let's go inside and look how we live. #laugh#</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a101" ANNOTATION_REF="a43">
                <ANNOTATION_VALUE>And this is called ## here.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a102" ANNOTATION_REF="a44">
                <ANNOTATION_VALUE>We got some firewood in case of the bad weather, so we don't have to walk in the snow.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a103" ANNOTATION_REF="a45">
                <ANNOTATION_VALUE>Here we have our upper room, in the olden time there was many people in every family. Three families lived in every little house.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a104" ANNOTATION_REF="a46">
                <ANNOTATION_VALUE>This is why we need the upper room, you see, here's all the goods, how they say.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a105" ANNOTATION_REF="a47">
                <ANNOTATION_VALUE>In the old days we kept seeds here.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a106" ANNOTATION_REF="a48">
                <ANNOTATION_VALUE>In our village.. It's about 70... It's probably less than a thousand.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a107" ANNOTATION_REF="a50">
                <ANNOTATION_VALUE>It was two kolkhoz before.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a108" ANNOTATION_REF="a51">
                <ANNOTATION_VALUE>Now it's next to nothing.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a109" ANNOTATION_REF="a52">
                <ANNOTATION_VALUE>The old people stay, there is no young people.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a110" ANNOTATION_REF="a53">
                <ANNOTATION_VALUE>You can't find the participants of this fire, those, who were alive then.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a111" ANNOTATION_REF="a55">
                <ANNOTATION_VALUE>## Only those who were just born at that time are alive now.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a112" ANNOTATION_REF="a56">
                <ANNOTATION_VALUE>The fire was so big, they sent a steam locomotive from Tula.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a113" ANNOTATION_REF="a57">
                <ANNOTATION_VALUE>They got children, old people and went to Tula.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a114" ANNOTATION_REF="a58">
                <ANNOTATION_VALUE>But there was a bridge on the road, the fire took the bridge, and the whole locomotive burnt.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a115" ANNOTATION_REF="a59">
                <ANNOTATION_VALUE>Everyone burnt to death.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a116" ANNOTATION_REF="a60">
                <ANNOTATION_VALUE>No one survived, they just couldn't.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
    </TIER>
